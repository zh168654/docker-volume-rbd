{
    "description": "RBD plugin for Docker",
    "documentation": "https://github.com/wetopi/docker-volume-rbd",
    "entrypoint": [
        "/docker-volume-rbd"
    ],
    "env": [
        {
            "name": "PLUGIN_VERSION",
            "Description": "Current version of RBD plugin for Docker Plugin",
            "settable": [
                "value"
            ],
            "value": "0.2.3"
        },
        {
            "name": "LOG_LEVEL",
            "Description": "[0:ErrorLevel; 1:WarnLevel; 2:InfoLevel; 3:DebugLevel] defaults to 0",
            "settable": [
                "value"
            ],
            "value": "0"
        },
        {
            "name": "ETCD_ENDPOINTS",
            "Description": "HTTP API address to the Etcd agent. i.e. 127.0.0.1:2375",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_MAP_DEVICE_ROOT",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_CLUSTER",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_KEYRING_USER",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_KEYRING_KEY",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_KEYRING_CAPS_MDS",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_KEYRING_CAPS_MON",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_KEYRING_CAPS_OSD",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_GLOBAL_FSID",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_GLOBAL_MON_INITIAL_MEMBERS",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_GLOBAL_MON_HOST",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_GLOBAL_AUTH_CLUSTER_REQUIRED",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_GLOBAL_AUTH_SERVICE_REQUIRED",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_GLOBAL_AUTH_CLIENT_REQUIRED",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_GLOBAL_OSD_POOL_DEFAULT_SIZE",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_GLOBAL_PUBLIC_NETWORK",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_CLIENT_RBD_DEFAULT_FEATURES",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_MDS_SESSION_TIMEOUT",
            "settable": [
                "value"
            ]
        },
        {
            "name": "RBD_CONF_MDS_SESSION_AUTOCLOSE",
            "settable": [
                "value"
            ]
        }
    ],
    "interface": {
        "socket": "rbd.sock",
        "types": [
            "docker.volumedriver/1.0"
        ]
    },
    "linux": {
        "AllowAllDevices": true,
        "capabilities": [
            "CAP_SYS_ADMIN"
        ],
        "devices": null
    },
    "mounts": [
        {
            "source": "/lib/modules",
            "destination": "/lib/modules",
            "type": "bind",
            "options": [
                "rbind"
            ]
        },
        {
            "source": "/dev",
            "destination": "/dev",
            "type": "bind",
            "options": [
                "rbind"
            ]
        },
        {
            "source": "/sys",
            "destination": "/sys",
            "type": "bind",
            "options": [
                "rbind"
            ]
        }
    ],
    "network": {
        "type": "host"
    },
    "propagatedmount": "/mnt/volumes"
}

